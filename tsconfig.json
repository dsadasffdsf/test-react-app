{
    "compilerOptions": {
      "target": "ES2017",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "jsx": "react",
      "allowImportingTsExtensions": true,
      "noEmit": true 
    },
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "types/**/*.d.ts",
    ],
    "exclude": [
      "node_modules"
    ]
  }